@model PagedList<Pro2Page>
@{
    Layout = "~/Areas/Wholesaler/Views/Shared/_Layout.cshtml";
}
<div class="h60"></div>
<div class="part">
    <p class="position">◆当前位置：微营销</p>
    <div class="admin_part">
        <table class="table" id="MemberTable">
            <thead>
                <tr>
                    <th>#序号</th>
                    <th>线路名称</th>
                    <th>预览连接</th>
                    <th>发布公司</th>
                    <th>相应操作</th>
                </tr>
            </thead>
            <tbody class="fs08 tcenter">
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Product.Name</td>
                        <td>@item.Company.CompanyName</td>
                        <td><a href="/Home/YuLan/@item.PageId" target="_blank">点击预览</a></td>
                        <td>
                            <a href="/Home/YuLan/@item.PageId" target="_blank">复制地址</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

