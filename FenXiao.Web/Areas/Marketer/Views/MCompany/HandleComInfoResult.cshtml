@model FenXiao.Model.HandleApply
@section title{
    <title>零售商-公司管理-消息中心</title>
}
<div class="h60"></div>
<div class="part">
    @if (Model.State == (int)EnumMessageState.tongguo)
    {
        <span>成功成为批发商，登陆总账号和当前总账号一致。管理用户可以正常使用</span>
    }
    else
    {
        <h3>申请被拒绝</h3>
        <p>@Model.Reason</p>
    }
    </div>
    @section scripts{
<script type="text/javascript">
    LeftMenuSelect(5);
</script>

    }
